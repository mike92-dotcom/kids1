pipeline {
    agent any
    stages {
        stage('Build Docker Image') {
            steps {
                sh 'docker build -t devops-app .'
            }
        }
        stage('Push Image') {
            steps {
                sh 'docker tag devops-app yourdockerhub/devops-app'
                sh 'docker push yourdockerhub/devops-app'
            }
        }
    }
}
